<?xml version="1.0" encoding="UTF-8"?>
<test-data>
	<UiMap>
		<pages>
            <page name="page_agreements" description = "Default first page." >
                <element name="checkbox_accept">
                    <id>com.hp.printercontrol:id/moobe_review_accept_agreements_checkbox</id>
                </element>
                <element name="button_continue">
                    <id>com.hp.printercontrol:id/moobe_welcome_continue_button</id>
                </element>
			</page>
			<page name="page_welcome" description="Welcome page">
                <element name="button_start" page="page_foundYourPrinter">
                    <id>com.hp.printercontrol:id/moobe_welcome_start_button</id>
                </element>
                <element name="link_setupLater">
                     <id>com.hp.printercontrol:id/moobe_setup_later</id>
                </element>
            </page>
            <page name="page_findNewPrinter" description="We find new printer.">
                <element name="button_continue" >
                    <id>com.hp.printercontrol:id/moobe_welcome_continue_button</id>
                </element>
			</page>
            <page name="page_foundANetworkPrinter" description="We found a network printer!">
                <element name="button_continue" >
                    <id>com.hp.printercontrol:id/moobe_welcome_continue_button</id>
                </element>
			</page>
            <page name="sys_packageinstaller" description="for android 6.0 'Allow xxxx to access device's location'.">
                <element name="link_AllowAccessLocation" >
                    <id>com.android.packageinstaller:id/permission_allow_button</id>
                </element>
			</page>
            <page name="page_foundYourPrinter" description="We found your printer.">
                <element name="link_changePrinter" page="page_printerList"><id>com.hp.printercontrol:id/change_printer</id></element>
                <element name="button_continue" page="page_printerList"><id>com.hp.printercontrol:id/moobe_welcome_continue_button</id></element>
                <element name="button_later" ><id>com.hp.printercontrol:id/moobe_welcome_later_button</id></element>
			</page>
            <page name="page_setup" description="set up printer.">
                <element name="button_ExitSetup" >
                    <id>com.hp.printercontrol:id/ap_selection_printer_not_present</id>
                </element>
			</page>
            <page name="page_printerList" description="Printer list.">
                <element name="button_search"><id>android:id/search_button</id></element>
                <element name="Edit_search"><id>android:id/search_src_text</id></element>
                <element name="text_printerIp" page="page_home"><xpath>//android.widget.TextView[contains(@text,'VALUE_PLACEHOLDER')]</xpath></element>
			</page>
            <page name="page_home" description="main page.">
                <element name="link_selectAPrinter" page="page_printerList"><id>com.hp.printercontrol:id/error_msg_link</id></element>
                <element name="button_scan"><id>com.hp.printercontrol:id/scan_button</id></element>
                <element name="button_capture"><id>com.hp.printercontrol:id/capture_button</id></element>
                <element name="button_file" ><id>com.hp.printercontrol:id/file_button</id></element>
                <element name="button_photos"><id>android:id/photos_button</id></element>
                <element name="button_printerInfo"><id>com.hp.printercontrol:id/printer_info_button</id></element>
                <element name="tile_printPhotos"><xpath>//android.widget.TextView[@text = 'Print Photos']</xpath></element>
                <element name="button_MoreOptions"><accessibility_id>More options</accessibility_id></element>
                <page name="menu_moreOptions">
                    <element name="menuItem_appSettings_"><xpath>//android.widget.TextView[@text = 'App Settings']</xpath></element>
                    <element name="menuItem_giveFeedback_"><xpath>//android.widget.TextView[@text = 'Give Feedback']</xpath></element>
                    <element name="menuItem_about_">
                        <xpath>//android.widget.TextView[@text = 'About']</xpath>
                    </element>
                    <element name="menuItem_showWelcomeScreen_"><xpath>//android.widget.TextView[@text = 'Show Welcome screen']</xpath></element>
                    <element name="menuItem_devInvestigations_"><xpath>//android.widget.TextView[@text = 'Dev Investigations']</xpath></element>
                </page>
			</page>
            <page name="page_about" description="about page.">
                <element name="text_versionLable" page="page_printerList"><id>com.hp.printercontrol:id/version_label</id></element>
                <element name="text_title_about"><id>android:id/action_bar_title</id></element>
                <element name="text_version"><id>com.hp.printercontrol:id/version</id></element>
                <element name="button_back" page="page_home"><id>android:id/up</id></element>
                <element name="text_appName"><id>com.hp.printercontrol:id/app_name</id></element>
                <element name="image_appIcon"><id>com.hp.printercontrol:id/app_icon</id></element>
                <element name="text_copyRightCompany"><id>com.hp.printercontrol:id/editText1</id></element>
                <element name="link_hpOnlinePrivacyStatement" page="page_hpOnlinePrivacyStatement"><id>com.hp.printercontrol:id/moobe_online_privacy</id></element>
                <element name="link_endUserLicenseAgreement" page="page_endUserLicenseAgreement"><id>com.hp.printercontrol:id/moobe_online_eula</id></element>
                <element name="link_legalInformation" page="dialog_legalInformation"><id>com.hp.printercontrol:id/legalInfoTextView</id></element>
                <element name="button_shareThisApp" page="page_shareThisApp"><id>com.hp.printercontrol:id/button_mail</id></element>
			</page>
            <page name="page_appSettings" description="moreOptions then app settings page.">
                <!--index="0" means return all specified elements, here is return all checkboxes-->
                <element name="checkBox_All"><id index="0">android:id/checkbox</id></element>
                <element name="checkBox_usageTracking"><id index="1">android:id/checkbox</id></element>
                <element name="checkBox_hpSuppliesShopping"><id index="2">android:id/checkbox</id></element>
                <element name="checkBox_wirelessNetwork"><id index="3">android:id/checkbox</id></element>
			</page>
            <page name="dialog_legalInformation" description="dialog_legalInformation">
                <element name="text_title_legalInformation" ><id>android:id/alertTitle</id></element>
                <element name="area_content_legalInformation"><xpath>//android.widget.FrameLayout/android.webkit.WebView</xpath></element>
                <element name="button_ok" page="page_about"><id>android:id/button3</id></element>
			</page>
            <page name="page_endUserLicenseAgreement" description="page_endUserLicenseAgreement">
                <element name="text_404PageNotFound"><accessibility_id>404 Page Not Found</accessibility_id></element>
                <element name="image_logo" page="page_about">
                    <xpath>
                        //android.webkit.WebView/android.view.View[1]/android.view.View[1]/android.view.View[1]/android.view.View[2]/android.view.View[1][contains(@content-desc,'HP® Official Site')]
                    </xpath>
                    <xpath>
                        //android.view.View[1]/android.view.View[1]/android.view.View[2]/android.view.View[1][contains(@content-desc,'HP® Official Site')]
                    </xpath>
                </element>
                <!--<element name="text_always" page="page_about"><id>android:id/button_always</id></element>-->
                <!--<element name="text_chrome" ><xpath>//android.widget.TextView[@text = 'Chrome']</xpath></element>-->
            </page>
            <page name="page_hpOnlinePrivacyStatement" description="page_hpOnlinePrivacyStatement">
                <element name="text_hpPrivacyStatementWorldwide"><xpath>//android.view.View[@content-desc = 'HP Privacy Statement Worldwide']</xpath></element>
                <element name="image_logo" page="page_about"><xpath>//android.widget.FrameLayout/android.webkit.WebView[1]/android.view.View[1]/android.view.View[1][@content-desc = 'HP']</xpath></element>
                <!--<element name="text_always" page="page_about"><id>android:id/button_always</id></element>-->
                <!--<element name="text_chrome" ><xpath>//android.widget.TextView[@text = 'Chrome']</xpath></element>-->
            </page>
            <page name="page_shareThisApp" description="page_shareThisApp">
                <element name="text_chooseAMailProvider"><xpath>//android.widget.TextView[@text = 'Choose a mail provider']</xpath></element>
                <element name="text_microsoftExchangeActiveSync"><xpath>//android.widget.TextView[@text = 'Microsoft Exchange ActiveSync']</xpath></element>
                <element name="text_sina" ><xpath>//android.widget.TextView[@text = 'Sina']</xpath></element>
                <element name="text_qq"><xpath>//android.widget.TextView[@text = 'QQ']</xpath></element>
                <element name="text_163" page="page_about"><xpath>//android.widget.TextView[@text = '163']</xpath></element>
                <element name="text_other" page="page_about"><xpath>//android.widget.TextView[@text = 'Other (POP3/IMAP)']</xpath></element>
			</page>
            <page name="sys_general" description="for android 6.0 'Allow xxxx to access device's location'.">
                <element name="link_AllowAccessLocation" >
                    <id>com.android.packageinstaller:id/permission_allow_button</id>
                </element>
                <element name="text_always" page="page_about"><id>android:id/button_always</id></element>
                <element name="text_chrome" ><xpath>//android.widget.TextView[@text = 'Chrome']</xpath></element>
                <element name="text_email" ><xpath>//android.widget.TextView[@text = 'Email']</xpath></element>
			</page>
		</pages>
	</UiMap>
</test-data>